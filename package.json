    "@ngx-share/buttons": "^5.0.1",
    "@ngx-share/core": "^5.0.1",
    "@types/hammerjs": "^2.0.35",
    "cfenv": "^1.0.4",
    "core-js": "^2.5.3",
    "font-awesome": "^4.7.0",
