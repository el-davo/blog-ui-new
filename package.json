{
  "name": "blog-ui",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "node": "8",
    "npm": "5"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build:ui": "ng build --prod -pr false",
    "build:server": "ng build --prod -pr false --output-hashing=false --app 1",
    "postbuild:server": "webpack --config webpack.server.config.js",
    "build": "npm run build:ui && npm run build:server && npm run workbox",
    "test": "echo \"Not yet implemented\"",
    "test:watch": "ng test -cc",
    "lint": "ng lint",
    "lint:fix": "npm run lint -- -fix",
    "e2e": "ng e2e --environment e2e",
    "prod": "node ./dist/server.js",
    "workbox": "workbox generate:sw",
    "bundle:size": "ng build --prod --stats-json && webpack-bundle-analyzer dist/browser/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0-rc.1",
