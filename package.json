{
  "name": "blog-ui",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "node": "8",
    "npm": "5"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build:ui": "ng build --prod -pr false",
    "build:server": "ng build --prod -pr false --output-hashing=false --app 1",
    "postbuild:server": "webpack --config webpack.server.config.js",
    "build": "npm run build:ui && npm run build:server && npm run sw",
    "test": "echo \"Not yet implemented\"",
    "test:watch": "ng test -cc",
    "lint": "ng lint",
    "lint:fix": "npm run lint -- -fix",
    "e2e": "ng e2e",
    "prod": "node ./dist/server.js",
    "sw": "sw-precache --root=dist/browser --config=sw-precache-config.js",
    "bundle:size": "ng build --prod --stats-json && webpack-bundle-analyzer dist/browser/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.1.1",
    "@angular/cdk": "^5.0.1",
    "@angular/common": "^5.1.1",
    "@angular/compiler": "^5.1.1",
    "@angular/core": "^5.1.1",
    "@angular/flex-layout": "^2.0.0-beta.12",
    "@angular/forms": "^5.1.1",
    "@angular/http": "^5.1.1",
    "@angular/material": "^5.0.1",
    "@angular/platform-browser": "^5.1.1",
    "@angular/platform-browser-dynamic": "^5.1.1",
    "@angular/platform-server": "^5.1.1",
    "@angular/router": "^5.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.5",
    "@ngx-share/button": "^5.0.0-beta.3",
    "@ngx-share/buttons": "^5.0.0-beta.3",
    "@ngx-share/core": "^5.0.0-beta.3",
    "cfenv": "^1.0.4",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "ngx-markdown": "^1.4.1",
    "redux-observable": "^0.14.1",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular-redux/store": "^6.5.4",
    "@angular/cli": "^1.6.3",
    "@angular/compiler-cli": "^5.1.2",
    "@angular/language-service": "^4.0.0",
    "@angularclass/hmr": "^2.1.3",
    "@types/jasmine": "2.5.45",
    "codelyzer": "~3.0.1",
    "concurrently": "^3.5.0",
    "faker": "^4.1.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "json-server": "^0.10.3",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "redux": "^3.7.1",
    "redux-immutable-state-invariant": "^2.0.0",
    "rosie": "^1.6.0",
    "sw-precache": "^5.2.0",
    "ts-loader": "^2.3.7",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "typescript": "^2.6.2",
    "webpack-bundle-analyzer": "^2.9.1"
  }
}
