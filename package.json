  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build --prod --progress false && ng run blog-ui:server --progress false && npm run webpack:server && npm run workbox",
    "webpack:server": "webpack --config webpack.server.config.js --progress false --colors",
    "build:server": "ng build --prod -pr false --output-hashing=false --app 1",
    "test": "echo \"Not yet implemented\"",
    "test:watch": "ng test -cc",
    "lint": "ng lint",
    "lint:fix": "npm run lint -- -fix",
    "e2e": "ng e2e --environment e2e",
    "prod": "node ./dist/server.js",
    "workbox": "workbox generate:sw",
    "bundle:size": "ng build --prod --stats-json && webpack-bundle-analyzer dist/browser/stats.json"
  },
  "private": true,
